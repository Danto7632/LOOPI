# Railway 환경변수 설정 템플릿
# Railway 대시보드에서 이 값들을 설정하세요

# 데이터베이스 설정 (Railway MySQL 사용시 자동 설정됨)
DATABASE_URL=${{DATABASE_URL}}
# 또는 개별 설정
DB_HOST=${{MYSQLHOST}}
DB_PORT=${{MYSQLPORT}}
DB_USERNAME=${{MYSQLUSER}}
DB_PASSWORD=${{MYSQLPASSWORD}}
DB_DATABASE=${{MYSQLDATABASE}}

# JWT 설정
JWT_SECRET=your-super-secret-jwt-key-for-production-at-least-32-characters
JWT_EXPIRES_IN=7d

# 애플리케이션 설정
NODE_ENV=production
PORT=${{PORT}}

# 파일 업로드 설정
MAX_FILE_SIZE=10485760

# CORS 설정 (프론트엔드 URL)
FRONTEND_URL=https://danto7632.github.io